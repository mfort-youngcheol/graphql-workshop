schema {
    mutation: Mutation
    query: Query
}

type Mutation {
    user(name: String!): User!
    card(userId: ID!, number: String!, company: String!): Card!
}

type Query {
    users: [User]
    cards: [Card]
}

type User {
    id: ID!
    name: String!
}

type Card {
    id: ID!
    number: String!
    company: String!
}
